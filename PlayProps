relabel_configs:
  - source_labels: [__address__]
    regex: '^([^.]+)\.[^.]+\.([^.]+)\.app\.example\.com:5550$'
    target_label: host
    replacement: '$1'  # Extracts 'dce-prd1-a'

  - source_labels: [host]
    regex: '^([^-]+-[^-]+)-[^-]+$'
    target_label: env
    replacement: '$1'  # Extracts 'dce-prd1'

  - source_labels: [__address__]
    regex: '^([^.]+)\.[^.]+\.([^.]+)\.app\.example\.com:5550$'
    target_label: region
    replacement: '$2'  # Extracts 'region'
