- name: Check if container ID file exists
  ansible.builtin.stat:
    path: /path/to/container.id
  register: container_id_file

- name: Read container ID if file exists
  ansible.builtin.command:
    cmd: cat /path/to/container.id
  register: container_id
  when: container_id_file.stat.exists
  changed_when: false # Reading file shouldn't count as a change
